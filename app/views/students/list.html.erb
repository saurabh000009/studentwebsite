<h1>Students</h1>

<ul>
<% @students  = Sboy.all %>
  <% @students.each do |student| %>
    <li>
      <a href="<%= student_path(student) %>">
        <%= student.name %>
      </a>
    </li>
  <% end %>
<ul>
  <% @students.each do |student| %>
    <li>
      <%= student.name %>
      <%= student.email %>
    </li>
  <% end %>
</ul> 


